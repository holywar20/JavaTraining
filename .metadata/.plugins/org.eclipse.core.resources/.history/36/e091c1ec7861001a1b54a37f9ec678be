package bwinter.training;

import java.util.ArrayList;


public class AutoBoxingLearn {
	String[] strArray = new String[10]; // NOT a primitive type. 
	int[] intArray = new int[10]; // This is a primitive type.
	
	ArrayList<String> strArrayList = new ArrayList<String>();
	ArrayList<String> intArrayList = new ArrayList<String>();
	
	for( int i = 0; i < intArray.length(); i++ ) {
		intArrayList.add( Integer.valueOf(i) );
	}
	
	
}
